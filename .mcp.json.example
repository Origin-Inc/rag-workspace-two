{
	"mcpServers": {
		"taskmaster-ai": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"--package=task-master-ai",
				"task-master-ai"
			],
			"env": {}
		},
		"supabase": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@supabase/mcp-server-supabase@latest",
				"--project-ref=YOUR_PROJECT_REF"
			],
			"env": {
				"SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
			}
		}
	}
}
