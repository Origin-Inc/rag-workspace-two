# LOCAL DEVELOPMENT ENVIRONMENT VARIABLES
# Copy this to .env.local for local development with local Supabase

# Local Supabase (npx supabase start)
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your-local-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-local-service-role-key

# Local Database (from npx supabase status)
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres
DIRECT_URL=postgresql://postgres:postgres@localhost:54322/postgres

# Keep these the same for local dev
NODE_ENV=development
APP_URL=http://localhost:3001

# Use your actual keys for these services
OPENAI_API_KEY=your-actual-openai-key
REDIS_URL=your-redis-url
SESSION_SECRET=your-session-secret
JWT_SECRET=your-jwt-secret